<resources>

    <!-- Base application theme, dependent on API level. This theme replaced by AppBaseTheme from res/value-vXX/styles.xml on newer devices. -->
    <style name="AppBaseTheme." parent="android:Theme.Light">
        <!-- Theme customization available in newer API level can go in res/values-vXX/styles.xml, while customization related to backward-compatibility can go here. -->
        </style>

        <!-- Application Theme. -->
        <style name="AppTheme" parent="AppBaseTheme">
            <!-- All customization that are NOT specified to a particular API-level can go here. -->
            <item name="android:windowNoTitle">true</item>
    </style>

</resources>
